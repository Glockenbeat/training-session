{
    "extends": "airbnb-base",
    "plugins": [
        "import",
        "json"
    ],
    "env": {
        "es6": true,
        "jest": true,
        "node": true
    },
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": ["error", { "ignore": ["^test", "^src","^shared"] }],
      "import/extensions" : 0,
      "comma-dangle":"warn",
      "jsx-a11y/no-static-element-interactions": "off",
      "no-console": 0,
      "import/first": 0,
      "import/prefer-default-export": 0,
      "react/forbid-prop-types": 0
    }
}
