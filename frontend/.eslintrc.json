{
    "parser": "babel-eslint",
    "extends": [
      "airbnb"
    ],
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "json"
    ],
    "env": {
        "es6": true,
        "jest": true,
        "node": true,
        "browser": true
    },
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": ["error", { "ignore": ["^test", "^src","^shared"] }],
      "import/extensions" : 0,
      "comma-dangle":"warn",
      "jsx-a11y/no-static-element-interactions": "off"
    }
}
